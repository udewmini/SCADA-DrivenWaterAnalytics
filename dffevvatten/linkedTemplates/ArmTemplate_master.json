{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dffevvatten"
		},
		"BlobStorageWaterProduction_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageWaterProduction'"
		},
		"DataLakeWaterProduction_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeWaterProduction'"
		},
		"DataLakeWaterProduction_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://waterproductiondata.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dffevvatten_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BlobStorageWaterProduction_connectionString": {
						"value": "[parameters('BlobStorageWaterProduction_connectionString')]"
					},
					"DataLakeWaterProduction_accountKey": {
						"value": "[parameters('DataLakeWaterProduction_accountKey')]"
					},
					"DataLakeWaterProduction_properties_typeProperties_url": {
						"value": "[parameters('DataLakeWaterProduction_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}